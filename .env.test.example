# Supabase Integration Test Configuration
# Copy this file to .env.test and fill in your test project credentials

# Test Supabase Project URL (separate from production)
SUPABASE_TEST_URL=https://your-test-project.supabase.co

# Test Supabase Anon Key
SUPABASE_TEST_ANON_KEY=your-test-anon-key

# Test User Credentials (for authenticated tests)
SUPABASE_TEST_USER_EMAIL=test@example.com
SUPABASE_TEST_USER_PASSWORD=test-password-123

# Skip integration tests (set to 'true' to run only unit tests)
SKIP_INTEGRATION_TESTS=false
