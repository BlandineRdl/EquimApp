{
  "name": "equim-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "npx biome check",
    "lint:fix": "npx biome check --write",
    "format": "npx biome format --write",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:production": "eas build --platform all --profile production"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "expo": "~53.0.20",
    "expo-constants": "^17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-linking": "^7.1.7",
    "expo-router": "^5.1.4",
    "expo-status-bar": "~2.2.3",
    "lucide-react-native": "^0.539.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "^2.1.4",
    "@expo/cli": "^0.24.20",
    "@types/react": "~19.0.10",
    "@types/react-redux": "^7.1.34",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4"
  },
  "private": true
}
